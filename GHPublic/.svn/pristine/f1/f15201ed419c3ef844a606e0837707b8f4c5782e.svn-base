<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/parent_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#ffffff"
        android:orientation="horizontal" >


        <TextView
            android:id="@+id/regist_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:text="完善资料"
            android:textColor="#1766b1"
            android:drawableLeft="@drawable/image_button_back"
            android:drawablePadding="16dp"
            android:textSize="18sp" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:layout_below="@+id/linearLayout1"
        android:src="@drawable/title_backgrundmenu" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="14dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="14dp"
            android:text="用户类型"
            android:textColor="#666666"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/regist_second_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="14dp"
            android:drawableRight="@drawable/ic_dropdown"
            android:text="船户"
            android:textColor="#333333"
            android:textSize="16dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d7d7d7"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="14dp"
        android:background="#d7d7d7"
        android:text="TextView" />

    <LinearLayout
        android:id="@+id/regist_pier"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingBottom="14dp" >

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:text="码头名称"
                android:textColor="#666666"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/wharfname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                android:background="#ffffff"
                android:hint="请输入码头名称"
                android:textColor="#333333"
                android:textSize="16sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingBottom="14dp" >

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:text="码头编号"
                android:textColor="#666666"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/wharfnum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                android:background="#ffffff"
                android:hint="请输入码头编号"
                android:textColor="#333333"
                android:textSize="16sp" >
            </EditText>
        </LinearLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingBottom="14dp" >

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:text="所在地区"
                android:textColor="#666666"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/regist_pier_area"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/ic_dropdown"
                android:hint="请选择所在地区"
                android:textColor="#333333"
                android:textSize="16sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="14dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="20dp"
                    android:text="添加照片(工商营业执照)"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/prompt2"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/ic_qusetion" />
            </LinearLayout>

            <!-- <ImageView -->
            <!-- android:id="@+id/imageView1" -->
            <!-- android:layout_width="wrap_content" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:layout_marginTop="20dp" -->
            <!-- android:layout_marginLeft="7dp" -->


            <!-- android:src="@drawable/ic_qusetion" /> -->

        </LinearLayout>
    </LinearLayout>
    <!-- 分割线 -->

    <LinearLayout
        android:id="@+id/regist_ship"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingBottom="14dp" >

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:text="船名号"
                android:textColor="#666666"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/shipname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                android:background="#ffffff"
                android:hint="请输入船名称"
                android:textColor="#333333"
                android:textSize="16sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingBottom="14dp" >

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:text="船舶登记号"
                android:textColor="#666666"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/shipnum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                android:background="#ffffff"
                android:hint="请输入船舶登记号"
                android:textColor="#333333"
                android:textSize="16sp" >
            </EditText>
        </LinearLayout>

        <TextView
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingBottom="14dp" >

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:text="所在地区"
                android:textColor="#666666"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/regist_ship_area"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="14dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/ic_dropdown"
                android:hint="请选择所在地区"
                android:textColor="#333333"
                android:textSize="16sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="14dp"
            android:background="#d7d7d7"
            android:text="TextView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="20dp"
                    android:text="添加照片(船舶国际证书)"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/prompt"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/ic_qusetion" />
            </LinearLayout>

            <!-- <ImageView -->
            <!-- android:id="@+id/imageView1" -->
            <!-- android:layout_width="wrap_content" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:layout_marginTop="20dp" -->
            <!-- android:layout_marginLeft="7dp" -->


            <!-- android:src="@drawable/ic_qusetion" /> -->

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:paddingBottom="22dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="22dp"
        android:orientation="horizontal" >

        <GridView
            android:id="@+id/add_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:columnWidth="80dp"
            android:horizontalSpacing="5dp"
            android:numColumns="4"
            android:verticalSpacing="5dp" >
        </GridView>
    </LinearLayout>

    <Button
        android:id="@+id/regist_submit"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/ic_selector_btn"
        android:text="完成注册"
        android:textColor="#ffffff" />

</LinearLayout>