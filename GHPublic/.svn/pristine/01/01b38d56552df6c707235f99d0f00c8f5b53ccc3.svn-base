<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/father_ship_circle"
    android:orientation="vertical"
    android:background="#f5f5f5" >
    
      <RelativeLayout
         android:id="@+id/relative_title"
        android:layout_width="match_parent"
        android:layout_height="48dip"
        android:background="#ffff" >

        <RelativeLayout
            android:id="@+id/relative_title_final"
            android:layout_width="wrap_content"
            android:layout_height="48dip" >

        <ImageView
            android:id="@+id/back"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/image_button_back" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@+id/back"
            android:text="船货圈"
            android:textColor="#1766b1"
            android:textSize="18sp" />
        
        </RelativeLayout>
        <ImageButton 
            android:id="@+id/image_add"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/ic_add_pic"/>
        
        

        
    </RelativeLayout>
    
       <ImageView
        android:id="@+id/imageView2"
        android:layout_below="@+id/relative_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@drawable/title_backgrundmenu" />

    

    <TextView
        android:id="@+id/aaa"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d7d7d7"
        android:text="TextView" />
   
    
    
    
    <ListView 
        android:id="@+id/lv_ship_circle"
        android:layout_width="match_parent"
        android:divider="#d7d7d7"
        android:dividerHeight="1px"
        android:background="#ffffff"
        android:layout_height="wrap_content">
        
    </ListView>

</LinearLayout>
