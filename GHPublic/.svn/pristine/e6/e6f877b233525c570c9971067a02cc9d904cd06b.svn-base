<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5" 
    android:orientation="vertical" >
    
     <RelativeLayout
         android:id="@+id/relative_title"
        android:layout_width="match_parent"
        android:layout_height="48dip"
        android:background="#ffff" >

        <RelativeLayout
            android:id="@+id/relative_title_final"
            android:layout_width="wrap_content"
            android:layout_height="48dip" >

        <ImageView
            android:id="@+id/back"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/image_button_back" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@+id/back"
            android:text="我的"
            android:textColor="#1766b1"
            android:textSize="18sp" />
        
        </RelativeLayout>
        <ImageButton 
            android:id="@+id/btn_setting"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/ic_wode_shezhi"/>

        
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_below="@+id/relative_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@drawable/title_backgrundmenu" />

    

    <TextView
        android:id="@+id/aaa"
        android:layout_width="match_parent"
        android:layout_height="16dp"
        android:background="#f5f5f5"
       />
    
    <ListView
        android:id="@+id/my_ship_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_weight="1.0"
        android:divider="#f5f5f5"
        android:dividerHeight="16dp" >
        
    </ListView>
    
    <RelativeLayout 
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
    	android:layout_marginBottom="20dp"
        >
        <Button
	    android:id="@+id/btn_add"
	    android:layout_width="match_parent"
	    android:layout_height="56dp"	    
	    android:layout_marginLeft="16dp"
	    android:layout_marginRight="16dp"
	    
	    android:background="@drawable/ic_selector_btn"
	    android:layout_marginTop="20dp"
	    android:textSize="18sp"
	    android:textColor="#ffff"
	    android:text="添加" />
    </RelativeLayout>
    
    

</LinearLayout>
