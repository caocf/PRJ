<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
   <class name="com.huzhouport.dangerousGoodsJob.model.WharfJobNum" table="mm_WharfJobNum" >
        <id name="wharfID" column="WharfID" >
            <generator class="native"></generator>
        </id>
 
        <property name="wharfName" column="WharfName" ></property>
        <property name="wharfLocation" column="WharfLocation" ></property>
        <property name="num" column="Num" ></property>    
   
    
    
    </class>
</hibernate-mapping>
