<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
   <class name="com.huzhouport.wharfWork.model.WharfWork" table="mm_WharfWork" >
   <id name="id" column="ID" >
            <generator class="native"></generator>
        </id>
         <property name="wharfWorkID" column="WharfWorkID" ></property>
        <property name="shipName" column="ShipName" ></property>
        <property name="startingPort" column="StartingPort" ></property>
        <property name="arrivalPort" column="ArrivalPort" ></property>
        <property name="portMart" column="PortMart" ></property>
        <property name="cargoTypes" column="CargoTypes" ></property>
        <property name="carrying" column="Carrying" ></property>
         <property name="uniti" column="Uniti" ></property>
        <property name="workTime" column="WorkTime" ></property>
        <property name="locationID" column="LocationID" ></property>
        <property name="workPhoto" column="WorkPhoto" ></property>
     <property name="declareTime" column="DeclareTime" ></property>
     <property name="name" column="Name" ></property>
    </class>
</hibernate-mapping>
