<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.electricreport.model.BoatmanKind" table="mm_BoatmanKind">
		<id name="boatmanKindID" column="BoatmanKindID">
			<generator class="native"></generator>
		</id>
		<property name="boatmanKindName" column="BoatmanKindName"></property>
		<property name="isDelete" column="IsDelete"></property>
	</class>
</hibernate-mapping>
