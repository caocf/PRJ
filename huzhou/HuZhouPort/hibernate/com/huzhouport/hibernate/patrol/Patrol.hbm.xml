<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.patrol.model.Patrol" table="mm_Patrol">
		<id name="patrolId" column="PatrolID">
			<generator class="native"></generator>
		</id>
		<property name="patrolUser" column="PatrolUser"></property>
		<property name="patrolObject" column="PatrolObject"></property>
		<property name="patrolContent" column="PatrolContent"></property>
		<property name="patrolTime" column="PatrolTime"></property>
		<property name="patrolLocation" column="PatrolLocation"></property>
		<property name="patCategoriese" column="PatrolCategoriese"></property>
	</class>
</hibernate-mapping>
