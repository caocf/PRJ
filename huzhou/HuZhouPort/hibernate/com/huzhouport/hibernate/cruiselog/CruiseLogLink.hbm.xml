<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.cruiselog.model.CruiseLogLink" table="mm_CruiseLogLink">
		<composite-id>
            <key-property name="cruiseLogID" column="CruiseLogID" />
            <key-property name="illegalID" column="IllegalID" />
        </composite-id>
	</class>
</hibernate-mapping>
