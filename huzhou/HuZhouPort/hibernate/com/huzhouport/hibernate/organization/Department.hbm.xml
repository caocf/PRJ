<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
    <class name="com.huzhouport.organization.model.Department" table="mm_Department">
        <id name="departmentId" column="DepartmentID">
            <generator class="native"></generator>
        </id>
        <property name="departmentName" column="DepartmentName"></property>
        <property name="partOfDepartmentId" column="PartOfDepartmentID"></property>
        <property name="refDepartID" column="RefDepartID"></property>
        
    </class>
</hibernate-mapping>
