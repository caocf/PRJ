<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.electric.model.ElectricVisa" table="mm_Visa" >
		<id name="visaID" column="VisaID">
			<generator class="native"></generator>
		</id>
		<property name="shipName" column="ShipName"></property>
		<property name="startingPort" column="StartingPort"></property>
		<property name="arrivalPort" column="ArrivalPort"></property>
		<property name="mark" column="Mark"></property>
		<property name="time" column="Time"></property>
		<property name="cargoTypes" column="CargoTypes"></property>
		<property name="visaStatus" column="VisaStatus"></property>
		<property name="carrying" column="Carrying"></property>
		<property name="visaUser" column="VisaUser"></property>
		<property name="visaUserName" column="VisaUserName"></property>
		<property name="visaContent" column="VisaContent"></property>
	</class>
</hibernate-mapping>
