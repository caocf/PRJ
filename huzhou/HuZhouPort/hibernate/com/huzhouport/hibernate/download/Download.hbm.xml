<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.download.model.Download" table="mm_download">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="appId" column="appId"></property>
		<property name="ip" column="downloadIp"></property>
		<property name="date" column="downTime"></property>
		<property name="appVersion" column="appVersion"></property>

	</class>
</hibernate-mapping>
