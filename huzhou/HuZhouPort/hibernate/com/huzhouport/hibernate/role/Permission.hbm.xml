<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.role.model.Permission" table="mm_Permission">
		<id name="permissionId" column="PermissionID">
			<generator class="native"></generator>
		</id>
		<property name="permissionName" column="PermissionName"></property>
		<property name="partOfKind" column="PartOfKind"></property>
	</class>
</hibernate-mapping>
