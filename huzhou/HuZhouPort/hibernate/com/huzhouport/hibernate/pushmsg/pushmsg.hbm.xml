<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.pushmsg.model.PushMsg" table="mm_pushmsg">
		<id name="pushmsgid" column="pushmsgid">
			<generator class="native"></generator>
		</id>
		<property name="userid" column="userid"></property>
		<property name="messageid" column="messageid"></property>
		<property name="modulerid" column="modulerid"></property>
		<property name="msgstatus" column="msgstatus"></property>
		<property name="pushmsgtime" column="pushmsgtime"></property>	
	</class>
</hibernate-mapping>
