<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.inspection.model.InspectionSupplement" table="mm_InspectionSupplement">
		<composite-id>
            <key-property name="inspectionId" column="InspectionID" />
            <key-property name="userId" column="UserID" />
        </composite-id>
        <property name="supplementTime" column="SupplementTime"></property>
	</class>
</hibernate-mapping>
