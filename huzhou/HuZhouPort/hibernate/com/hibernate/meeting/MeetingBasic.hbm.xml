<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.meeting.model.MeetingBasic" table="meeting_basic">
		<id name="id" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="applicaionor" column="applicaionor" not-null="false"></property>
		<property name="applytime" column="applytime" not-null="false"></property>
		<property name="meetingroom" column="meetingroom" not-null="false"></property>
		<property name="topic" column="topic" not-null="false"></property>
		<property name="meetingdate" column="meetingdate" not-null="false"></property>
		<property name="meetingtime" column="meetingtime" not-null="false"></property>
		<property name="tel" column="tel" not-null="false"></property>
		<property name="approver" column="approver" not-null="false"></property>
		<property name="approvetime" column="approvetime" not-null="false"></property>
			<property name="approvemark" column="approvemark" not-null="false"></property>
		<property name="officeapprover" column="officeapprover" not-null="false"></property>
		<property name="depaptime" column="depaptime" not-null="false"></property>
		<property name="depapmark" column="depapmark" not-null="false"></property>
		<property name="status" column="status" not-null="false"></property>
		<property name="depstatus" column="depstatus" not-null="false"></property>
		<property name="isread" column="isread" not-null="false"></property>
	</class>
</hibernate-mapping>
