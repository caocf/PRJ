<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<!-- Action必须放在指定的包名空间中 -->
	<package name="modelPortNotice" extends="cement-interceptor" namespace="/">

		<action name="captureHtmlPortNotice" class="com.huzhouport.portNotice.action.ModelActionPortNotice"
			method="captureHtml">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		
		<action name="captureHtmlPortNotice1" class="com.huzhouport.portNotice.action.ModelActionPortNotice"
			method="captureHtml1">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="findDetailInfoPortNotice" class="com.huzhouport.portNotice.action.ModelActionPortNotice"
			method="findDetailInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>

	</package>

</struts>    
