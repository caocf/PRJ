<struts>

	<!-- Action必须放在指定的包名空间中 -->
	<package name="portDynmicNews" extends="cement-interceptor" namespace="/">

		<action name="captureHtml" class="com.huzhouport.portDynmicNews.action.ModelActionPortDynmicNews"
			method="captureHtml">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="captureHtml1" class="com.huzhouport.portDynmicNews.action.ModelActionPortDynmicNews"
			method="captureHtml1">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="findDetailInfo" class="com.huzhouport.portDynmicNews.action.ModelActionPortDynmicNews"
			method="findDetailInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="findTopImageInfo" class="com.huzhouport.portDynmicNews.action.ModelActionPortDynmicNews"
			method="findTopImageInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		

	</package>

</struts>    
