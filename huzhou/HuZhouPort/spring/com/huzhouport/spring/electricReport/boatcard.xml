<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="boatcardDao"
		class="com.huzhouport.electricreport.dao.impl.BoatcardDaoImpl">
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>

	<bean id="boatcardService"
		class="com.huzhouport.electricreport.service.impl.BoatcardServiceImpl">
		<property name="boatcardDao" ref="boatcardDao"></property>
	</bean>

	<bean name="BoatcardAction"
		class="com.huzhouport.electricreport.action.BoatcardAction">
		<property name="boatcardService" ref="boatcardService"></property>
	</bean>

</beans>
