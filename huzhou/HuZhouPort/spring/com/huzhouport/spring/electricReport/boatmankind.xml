<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="boatmanKindDao"
		class="com.huzhouport.electricreport.dao.impl.BoatmanKindDaoImpl">
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>

	<bean id="boatmanKindService"
		class="com.huzhouport.electricreport.service.impl.BoatmanKindServiceImpl">
		<property name="boatmanKindDao" ref="boatmanKindDao"></property>
	</bean>

	<bean name="BoatmanKindAction"
		class="com.huzhouport.electricreport.action.BoatmanKindAction">
		<property name="boatmanKindService" ref="boatmanKindService"></property>
	</bean>

</beans>
