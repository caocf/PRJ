<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           


   <bean id="dangerousgoodsjobdao" class="com.huzhouport.dangerousGoodsJob.dao.impl.DaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>	
  

	<bean id="dangerousGoodsJobServer" class="com.huzhouport.dangerousGoodsJob.service.impl.DangerousGoodsJobServerImpl">
		<property name="dao" ref="dangerousgoodsjobdao"></property>
	</bean>
	
	
<bean name="DangerousGoodsPortlnAction" class="com.huzhouport.dangerousGoodsJob.action.DangerousGoodsJobAction" scope="prototype"   >
		<property name="dangerousGoodsJobServer" ref="dangerousGoodsJobServer"></property>
	
	</bean> 




</beans>
