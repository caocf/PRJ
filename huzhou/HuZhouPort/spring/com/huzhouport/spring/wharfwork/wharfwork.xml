<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           


   <bean id="wharfworkdao" class="com.huzhouport.wharfWork.dao.impl.WharfWorkDaoImpl">
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>	

	<bean id="wharfWorkServer" class="com.huzhouport.wharfWork.service.impl.WharfWorkServerImpl">
		<property name="wharfworkdao" ref="wharfworkdao"></property>
	</bean>
	
	
<bean name="WharfWorkAction" class="com.huzhouport.wharfWork.action.WharfWorkAction"  scope="prototype">
		<property name="wharfWorkServer" ref="wharfWorkServer"></property>
        <property name="logsaveserver" ref="logsaveserver"></property>
	</bean> 




</beans>
