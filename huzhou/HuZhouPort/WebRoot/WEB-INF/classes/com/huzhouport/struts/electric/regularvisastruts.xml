<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<!-- Action必须放在指定的包名空间中 -->
	<package name="regularVisa" extends="cement-interceptor" namespace="/">
	<!-- 增加 -->
		<action name="findRegularVisaInfo" class="com.huzhouport.electric.action.RegularVisaAction"
			method="findRegularVisaInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
	
		<!-- 增加 -->
		<action name="addRegularVisaInfo" class="com.huzhouport.electric.action.RegularVisaAction"
			method="addRegularVisaInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="showRegularVisaID" class="com.huzhouport.electric.action.RegularVisaAction"
			method="showRegularVisaID">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		
		</package>

</struts>    
