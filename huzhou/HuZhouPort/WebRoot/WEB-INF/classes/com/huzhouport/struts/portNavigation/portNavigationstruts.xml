<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<!-- Action必须放在指定的包名空间中 -->
	<package name="modelPortNavigation" extends="cement-interceptor" namespace="/">

		<action name="captureHtmlPortNavigation" class="com.huzhouport.portNavigation.action.ModelActionNavigation"
			method="captureHtml">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		
		<action name="captureHtmlPortNavigation1" class="com.huzhouport.portNavigation.action.ModelActionNavigation"
			method="captureHtml1">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="findDetailInfoPortNavigation" class="com.huzhouport.portNavigation.action.ModelActionNavigation"
			method="findDetailInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>

	</package>

</struts>    
