<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<!-- Action必须放在指定的包名空间中 -->
	<package name="fixedTermReport" extends="cement-interceptor"
		namespace="/">

		<!-- 查询信息 -->
		<action name="findFixedTermReportInfo"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="findFixedTermReportInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<!-- 修改信息 -->
		<action name="modifyFixedTermReportInfo"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="modifyFixedTermReportInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="deleteFixedTermReportInfo"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="deleteFixedTermReportInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<!-- 增加 -->
		<action name="addFixedTermReportInfo"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="addFixedTermReportInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<!-- 删除 -->
		<action name="deleteFixedTermReportInfo"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="deleteFixedTermReportInfo">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<!-- 按ID-->
		<action name="findFixedTermReportInfoById"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="findFixedTermReportInfoById">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="findRegularVisaById"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="findRegularVisaById">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>
		<action name="findFixedReportInfoById"
			class="com.huzhouport.electric.action.FixedTermReportAction" method="findFixedReportInfoById">
			<result name="success" type="json" />
			<result name="error">/error.jsp</result>
		</action>

	</package>

</struts>    
