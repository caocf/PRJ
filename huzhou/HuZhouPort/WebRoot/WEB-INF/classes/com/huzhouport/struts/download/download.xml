<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="download" extends="cement-interceptor"
		namespace="/">
		<action name="queryLastVersion"
			class="com.huzhouport.download.action.DownloadAction"
			method="queryLastVersion">
			<result name="success" type="json" ><param name="root">version</param></result>
			<result name="error"/>
		</action>
		
		<action name="downLastApp"
			class="com.huzhouport.download.action.DownloadAction"
			method="downLastApp">
			<result name="success" type="redirect">${directURL}</result>
			<result name="error"/>
		</action>

		<action name="queryAppCount"
			class="com.huzhouport.download.action.DownloadAction"
			method="queryAppCount">
			<result name="success" type="json" ></result>
			<result name="error"/>
		</action>
	</package>
</struts>