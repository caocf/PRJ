<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<!-- Action必须放在指定的包名空间中 -->
	<package name="homePage" extends="cement-interceptor" namespace="/">
		<!--用户列表或搜索-->
		<action name="findHomePageInfo" class="com.huzhouport.user.action.HomePageAction"
			method="findHomePageInfo">
			<result name="success" type="json" />
		</action>
		<action name="findHomePageInfoPrompt" class="com.huzhouport.user.action.HomePageAction"
			method="findHomePageInfoPrompt">
			<result name="success" type="json" />
		</action>
	</package>
</struts>    
