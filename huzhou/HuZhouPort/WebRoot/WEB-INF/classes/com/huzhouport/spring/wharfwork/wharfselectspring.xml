<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="pieceAreaDao" class="com.huzhouport.wharfSelect.dao.PieceAreaDao">
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>
	<bean id="portAreaDao" class="com.huzhouport.wharfSelect.dao.PortAreaDao">
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>
	<bean id="wharfItemDao" class="com.huzhouport.wharfSelect.dao.WharfItemDao">
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>

	<bean id="wharfSelectService"
		class="com.huzhouport.wharfSelect.service.WharfSelectService">
		<property name="pieceAreaDao" ref="pieceAreaDao"></property>
		<property name="portAreaDao" ref="portAreaDao"></property>
		<property name="wharfItemDao" ref="wharfItemDao"></property>
	</bean>
	
	<bean name="wharfSelectAction" class="com.huzhouport.wharfSelect.action.WharfSelectAction"
		scope="prototype">
		<property name="wharfSelectService" ref="wharfSelectService"></property>
		
	</bean>
</beans>
