<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           


   <bean id="knowledgedao" class="com.huzhouport.knowledge.dao.impl.DaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>	
  

	<bean id="knowledgeServer" class="com.huzhouport.knowledge.service.impl.KnowLedgeServerImpl">
		<property name="dao" ref="knowledgedao"></property>
	</bean>
	
<bean name="KnowLedgeAction" class="com.huzhouport.knowledge.action.KnowLedgeAction"  scope="prototype"  >
		<property name="knowledgeServer" ref="knowledgeServer"></property>
	<property name="logsaveserver" ref="logsaveserver"></property>
	<property name="pushservice" ref="pushservice"></property>
	</bean> 




</beans>
