<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           


   <bean id="dangerousgoodsportlndao" class="com.huzhouport.dangerousgoodsportln.dao.impl.DaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
		<property name="hibernateTemplate">
			<ref bean="hibernateTemplate" />
		</property>
	</bean>	
  

	<bean id="dangerousGoodsPortlnServer" class="com.huzhouport.dangerousgoodsportln.service.impl.DangerousGoodsPortlnServerImpl">
		<property name="dao" ref="dangerousgoodsportlndao"></property>
	</bean>
	
	
<bean name="DangerousGoodsPortlnAction" class="com.huzhouport.dangerousgoodsportln.action.DangerousGoodsPortlnAction"  scope="prototype"  >
		<property name="dangerousGoodsPortlnServer" ref="dangerousGoodsPortlnServer"></property>
	     <property name="logsaveserver" ref="logsaveserver"></property>
	</bean> 




</beans>
