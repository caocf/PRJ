<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.aisno.model.AISNo" table="gz_aisno">
		<id name="aisnum" column="aisnum">
			<generator class="assigned" />
		</id>
		<property name="shipname" column="shipname"></property>
		<property name="type" column="cblx"></property>

	</class>
</hibernate-mapping>
