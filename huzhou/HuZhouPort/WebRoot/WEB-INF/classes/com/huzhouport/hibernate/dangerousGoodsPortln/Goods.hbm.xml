<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
   <class name="com.huzhouport.dangerousgoodsportln.model.Goods" table="mm_Goods" >
        <id name="goodsID" column="GoodsID" >
            <generator class="native"></generator>
        </id>
 
        <property name="goodsName" column="GoodsName" ></property>
        <property name="goodsKindID" column="GoodsKindID" ></property>

     
     
   
    
    
    </class>
</hibernate-mapping>
