<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.illegal.model.IllegalEvidenceLink" table="mm_IllegalEvidenceLink">
		<composite-id>
            <key-property name="illegalId" column="IllegalID" />
            <key-property name="evidenceId" column="EvidenceID" />
        </composite-id>
	</class>
</hibernate-mapping>
