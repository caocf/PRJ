<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.electricreport.model.ElectricArrival" table="mm_ElectricArrival">
		<id name="electId" column="ElectId">
			<generator class="native"></generator>
		</id>
		<property name="reportID" column="ReportID"></property>
		<property name="arrivalPort" column="ArrivalPort"></property>
		<property name="arrivalTime" column="ArrivalTime"></property>
	</class>
</hibernate-mapping>
