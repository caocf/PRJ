<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.publicuser.model.ShipBinding" table="mm_ShipBinding">
		<id name="shipId" column="ShipId">
			<generator class="native"></generator>
		</id>
		<property name="shipNum" column="ShipNum"></property>
		<property name="shipUser" column="ShipUser"></property>
		<property name="bindingTime" column="BindingTime"></property>
		<property name="status" column="Status"></property>
	</class>
</hibernate-mapping>
