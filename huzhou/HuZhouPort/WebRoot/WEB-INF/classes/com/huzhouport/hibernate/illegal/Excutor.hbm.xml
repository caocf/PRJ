<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象 
-->
<hibernate-mapping>
	<class name="com.huzhouport.illegal.model.Excutor" table="mm_excutor">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<!-- 定义其他属性 -->
		<property name="name" column="name" not-null="false"></property>
		<property name="pot" column="pot" not-null="false"></property>
		<property name="state" column="state" not-null="false"></property>
		<property name="potid" column="potid" not-null="false"></property>
	</class>
</hibernate-mapping>
