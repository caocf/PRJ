<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象
-->
<hibernate-mapping>
	<class name="com.huzhouport.schedule.model.ScheduleKind" table="mm_ScheduleKind">
		<id name="scheduleKindId" column="ScheduleKindID">
			<generator class="native"></generator>
		</id>
		<property name="scheduleKindName" column="ScheduleKindName"></property>
	</class>
</hibernate-mapping>
