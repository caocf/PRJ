<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!-- 
    每个class对应一个持久化对象 
-->
<hibernate-mapping>
	<class name="com.huzhouport.illegal.model.AbImage" table="mm_abimage">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<!-- 定义其他属性 -->
		<property name="abid" column="abid" not-null="false"></property>
		<property name="dir" column="dir" not-null="false"></property>
		
	</class>
</hibernate-mapping>
