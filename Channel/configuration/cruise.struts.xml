<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <package name="cruise" extends="cement-interceptor">
        <action name="querycruise" method="queryCruise"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="querycruisebyid" method="queryCruiseById"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="delcruise" method="delCruise"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="addcruise" method="addCruise"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="updatecruise" method="updateCruise"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="queryyhlxd" method="queryYhlxd"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="queryyhlxdbyid" method="queryYhlxdById"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="delyhlxd" method="delYhlxd"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="updateyhlxd" method="updateYhlxd"
                class="com.channel.cruise.CruiseAction">
            <result name="success" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="exportyhlxd" method="exportYhlxd"
                class="com.channel.cruise.CruiseAction">
            <result name="json" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
            <result name="file" type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="contentLength">${dlFilelength}</param>
                <param name="inputName">dlFile</param>
                <param name="contentDisposition">attachment;${dlFileName}</param>
                <param name="bufferSize">4096</param>
            </result>
        </action>
        <action name="downloadcxcfj" method="downloadCxcfj"
                class="com.channel.cruise.CruiseAction">
            <result name="json" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
            <result name="file" type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="contentLength">${dlFilelength}</param>
                <param name="inputName">dlFile</param>
                <param name="contentDisposition">attachment;${dlFileName}</param>
                <param name="bufferSize">4096</param>
            </result>
        </action>
        <action name="downloadcxcyhfj" method="downloadCxcyhfj"
                class="com.channel.cruise.CruiseAction">
            <result name="json" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
            <result name="file" type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="contentLength">${dlFilelength}</param>
                <param name="inputName">dlFile</param>
                <param name="contentDisposition">attachment;${dlFileName}</param>
                <param name="bufferSize">4096</param>
            </result>
        </action>
    </package>
</struts>

