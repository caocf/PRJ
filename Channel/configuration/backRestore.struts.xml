            <result name="json" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>

        <action name="restore" method="restore"
                class="com.channel.backrestore.BackRestoreAction">
            <result name="json" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
        <action name="backup" method="backup"
                class="com.channel.backrestore.BackRestoreAction">
            <result name="json" type="json">
                <param name="root">result</param>
                <param name="ignoreHierarchy">false</param>
            </result>
        </action>
    </package>
</struts>

