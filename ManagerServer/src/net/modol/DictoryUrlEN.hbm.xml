<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.DictoryUrlEN" table="dictory_url" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="editurl">
            <column name="editurl" sql-type="varchar(255)" not-null="false"/>
        </property>
        <many-to-one name="groupEN" lazy="false">
            <column name="groupid" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>