<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.ScheduleBaseEN" table="schedule_base" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="scheduletime">
            <column name="scheduletime"  not-null="false"/>
        </property>
        <property name="location">
            <column name="location" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="remind">
            <column name="remind" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="remindtype">
            <column name="remindtype" sql-type="varchar(255)" not-null="false"/>
        </property>
        <many-to-one name="user" class="net.modol.JcxxYhEN" column="userid" insert="true" update="true" lazy="false"/>
    </class>
</hibernate-mapping>