<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.UserRoleEN" table="user_role" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="role">
            <column name="role" sql-type="varchar(225)" length="225" not-null="true"/>
        </property>
        <set name="permissionENs" table="user_role_permit" cascade="all" inverse="false" lazy="false">
            <key column="roleid"></key>
            <many-to-many class="net.modol.UserPermissionEN" column="perid"/>
        </set>
    </class>
</hibernate-mapping>