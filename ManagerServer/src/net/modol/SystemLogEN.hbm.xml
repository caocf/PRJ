<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.SystemLogEN" table="system_log" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="op">
            <column name="op" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="optime">
            <column name="optime"  not-null="false"/>
        </property>
    </class>
</hibernate-mapping>