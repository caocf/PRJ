<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.LawSignedpdfEN" table="law_signedpdf" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="dir">
            <column name="dir" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="unsigneddir">
            <column name="unsigneddir" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>