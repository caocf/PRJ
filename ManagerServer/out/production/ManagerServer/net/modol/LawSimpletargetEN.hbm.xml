<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.LawSimpletargetEN" table="law_simpletarget" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="cert">
            <column name="cert" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="lawname">
            <column name="lawname" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="duty">
            <column name="duty" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="location">
            <column name="location" sql-type="varchar(255)" not-null="false"/>
        </property>

    </class>
</hibernate-mapping>