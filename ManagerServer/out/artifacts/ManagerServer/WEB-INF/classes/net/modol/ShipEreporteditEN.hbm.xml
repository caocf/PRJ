<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.ShipEreporteditEN" table="ship_ereportedit" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <many-to-one name="portEN" class="net.modol.CommonPortEN" lazy="false">
            <column name="endport"/>
        </many-to-one>
        <property name="uptime" column="uptime"></property>
        <property name="porttime" column="porttime"></property>
    </class>
</hibernate-mapping>