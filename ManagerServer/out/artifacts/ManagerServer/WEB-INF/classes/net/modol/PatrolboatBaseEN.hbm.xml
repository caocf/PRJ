<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.PatrolboatBaseEN" table="patrolboat_base" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="dep">
            <column name="dep" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="level">
            <column name="level" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="pid">
            <column name="pid" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="haschildren">
            <column name="haschildren" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="member">
            <column name="member" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="shiplength">
            <column name="shiplength" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="buildtime">
            <column name="buildtime" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="childrenum">
            <column name="childrenum" sql-type="varchar(255)" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>