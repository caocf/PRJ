<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.CruiseFileEN" table="cruise_file" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"></generator>
        </id>
        <property name="dir">
            <column name="dir" sql-type="varchar(255)" not-null="true"/>
        </property>
        <many-to-one name="cruiseLogEN" lazy="false">
        <column name="logid"   not-null="false"/>
    </many-to-one>

    </class>
</hibernate-mapping>