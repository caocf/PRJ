<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.UserBaseEN" table="user_base" schema="zjmanager">
        <id name="id">
            <column name="id"/>
            <generator class="increment"/>
        </id>

        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="psw">
            <column name="psw" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="dep">
            <column name="dep" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="level">
            <column name="level" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="pid">
            <column name="pid" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="haschildren">
            <column name="haschildren" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="expand">
            <column name="expand" sql-type="varchar(255)" not-null="false"/>
        </property>
        
    </class>
</hibernate-mapping>