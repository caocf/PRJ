<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.modol.LawEvidencesEN" table="law_evidences" schema="zjmanager">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="dir">
            <column name="dir" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="signedpdf">
            <column name="signedpdf" sql-type="varchar(255)" not-null="false"/>
        </property>
        <many-to-one name="lawBaseEN" class="net.modol.LawBaseEN" cascade="none" insert="true" update="true" lazy="false" >
            <column name="lawid"/>
                </many-to-one>
    </class>
</hibernate-mapping>