<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.ghpublic.modol.GateBaseEN" table="gate_base" schema="zjport">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="order">
            <column name="order" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="level1">
            <column name="level1" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="level2">
            <column name="level2" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="shipnum">
            <column name="shipnum" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="shipname">
            <column name="shipname" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>