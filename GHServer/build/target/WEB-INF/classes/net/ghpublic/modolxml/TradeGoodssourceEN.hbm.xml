<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.ghpublic.modol.TradeGoodssourceEN" table="trade_goodssource" schema="zjport">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="status">
            <column name="status" not-null="true"/>
        </property>
        <many-to-one name="userEN" class="net.ghpublic.modol.PublicUserEN" lazy="false">
            <column name="userid"   not-null="false"/>
        </many-to-one>
        <property name="titile">
            <column name="titile" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="postdate">
            <column name="postdate" not-null="false"/>
        </property>
        <many-to-one name="startport" class="net.ghpublic.modol.CommonPortEN" lazy="false">
            <column name="startport"   not-null="false"/>
        </many-to-one>
        <many-to-one name="unloadport" class="net.ghpublic.modol.CommonPortEN" lazy="false">
            <column name="unloadport"   not-null="false"/>
        </many-to-one>
        <property name="price">
            <column name="price" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="linkman">
            <column name="linkman" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="remark">
            <column name="remark" sql-type="varchar(255)" not-null="false"/>
        </property>
        <many-to-one name="tradeTypeEN" class="net.ghpublic.modol.TradeTypeEN" lazy="false">
            <column name="tradetypeid"   not-null="false"/>
        </many-to-one>

        <many-to-one name="type" class="net.ghpublic.modol.CommonGoodsunitEN" lazy="false">
            <column name="typeid" not-null="false"/>
        </many-to-one>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="tons">
            <column name="tons" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="packaging">
            <column name="packaging" sql-type="varchar(255)" not-null="false"/>
        </property>

        <many-to-one name="tradeShiptypeEN" class="net.ghpublic.modol.TradeShiptypeEN" lazy="false">
            <column name="shiptypeid" not-null="false"/>
        </many-to-one>
        <many-to-one name="unitEN" class="net.ghpublic.modol.CommonUnitEN" lazy="false">
            <column name="unitid" not-null="false"/>
        </many-to-one>
        <property name="shipname">
            <column name="shipname" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="load">
            <column name="loadnum" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="route">
            <column name="routeline" sql-type="varchar(255)" not-null="false"/>
        </property>

        <property name="age">
            <column name="agenum" sql-type="varchar(255)" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>