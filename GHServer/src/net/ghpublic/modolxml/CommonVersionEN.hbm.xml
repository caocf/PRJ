<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.ghpublic.modol.CommonVersionEN" table="common_version" schema="zjport">
        <id name="id">
            <column name="id" sql-type="int(11)" not-null="true"/>
            <generator class="increment"/>
        </id>
        <property name="versionNum">
            <column name="versionNum" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="verstionDec">
            <column name="verstionDec" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="uptime">
            <column name="uptime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="log">
            <column name="log" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="appType">
            <column name="appType" sql-type="varchar(255)" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>