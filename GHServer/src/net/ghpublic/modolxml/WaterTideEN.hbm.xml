<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.ghpublic.modol.WaterTideEN" table="water_tide" schema="zjport">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="port">
            <column name="port" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="freshdate">
            <column name="freshdate"  not-null="false"/>
        </property>
        <property name="standard">
            <column name="standard" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>