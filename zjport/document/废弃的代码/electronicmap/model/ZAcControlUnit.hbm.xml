<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zjport.model.ZAcControlUnit" table="z_ac_control_unit" schema="zjport">
        <composite-id>
            <key-property name="controlUnitId" column="CONTROL_UNIT_ID"/>
            <key-property name="indexCode" column="INDEX_CODE"/>
            <key-property name="name" column="NAME"/>
            <key-property name="parentId" column="PARENT_ID"/>
            <key-property name="parentTree" column="PARENT_TREE"/>
            <key-property name="unitLevel" column="UNIT_LEVEL"/>
            <key-property name="defaultRegionId" column="DEFAULT_REGION_ID"/>
            <key-property name="useStreamServer" column="USE_STREAM_SERVER"/>
            <key-property name="streamServerId" column="STREAM_SERVER_ID"/>
            <key-property name="description" column="DESCRIPTION"/>
            <key-property name="sequenceIdx" column="SEQUENCE_IDX"/>
            <key-property name="operatorId" column="OPERATOR_ID"/>
            <key-property name="createTime" column="CREATE_TIME"/>
            <key-property name="updateTime" column="UPDATE_TIME"/>
            <key-property name="cmsCascadeId" column="CMS_CASCADE_ID"/>
            <key-property name="cmsCascadeType" column="CMS_CASCADE_TYPE"/>
            <key-property name="shareLevel" column="SHARE_LEVEL"/>
            <key-property name="sequenceSort" column="SEQUENCE_SORT"/>
            <key-property name="useStreamStrictPath" column="USE_STREAM_STRICT_PATH"/>
            <key-property name="cascadeIndex" column="CASCADE_INDEX"/>
            <key-property name="controlOrder" column="CONTROL_ORDER"/>
        </composite-id>
    </class>
</hibernate-mapping>