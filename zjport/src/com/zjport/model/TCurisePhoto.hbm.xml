<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zjport.model.TCurisePhoto" table="t_curise_photo" schema="zjport">
        <id name="stCruisePhotoId" column="ST_CRUISE_PHOTO_ID">
            <generator class="native"/>
        </id>
        <property name="stCuriseMiddleId" column="ST_CURISE_MIDDLE_ID"/>
        <property name="stPhotoDescribe" column="ST_PHOTO_DESCRIBE"/>
        <property name="stCruiseAddress" column="ST_CRUISE_ADDRESS"/>
        <property name="dtCurise" column="DT_CURISE"/>
        <property name="stSource" column="ST_SOURCE"/>
    </class>
</hibernate-mapping>