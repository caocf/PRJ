<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zjport.model.TAddressListGroup" table="t_address_list_group" schema="zjport">
        <id name="stAddressListGroupId" type="java.lang.Integer">
            <column name="ST_ADDRESS_LIST_GROUP_ID"/>
            <generator class="identity"/>
        </id>
        <property name="stGroupName" type="java.lang.String">
            <column name="ST_GROUP_NAME"/>
        </property>
        <property name="stOrgId" type="java.lang.Integer">
            <column name="ST_ORG_ID"/>
        </property>
    </class>
</hibernate-mapping>