<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zjport.model.TCalendar" table="t_calendar" schema="zjport">
        <id name="stCalendarId" column="ST_CALENDAR_ID"/>
        <property name="stCalendarTitle" column="ST_CALENDAR_TITLE"/>
        <property name="stUserId" column="ST_USER_ID"/>
        <property name="dtStart" column="DT_START"/>
        <property name="dtEnd" column="DT_END"/>
        <property name="stContent" column="ST_CONTENT"/>
        <property name="stType" column="ST_TYPE"/>
        <property name="stCalendarMiddleId" column="ST_CALENDAR_MIDDLE_ID"/>
        <property name="stUrgentState" column="ST_URGENT_STATE"/>
        <property name="stAttachmentMiddleId" column="ST_ATTACHMENT_MIDDLE_ID"/>
        <property name="stIsAlert" column="ST_IS_ALERT"/>
    </class>
</hibernate-mapping>