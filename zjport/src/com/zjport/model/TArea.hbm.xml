<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zjport.model.TArea" table="t_area">
        <id name="stAreaCode" type="java.lang.String">
            <column name="ST_AREA_CODE" length="6" />
            <generator class="assigned" />
        </id>
        <property name="stAreaName" type="java.lang.String">
            <column name="ST_AREA_NAME" length="20" />
        </property>
        <property name="stPCode" type="java.lang.String">
            <column name="ST_P_CODE" length="6" />
        </property>
    </class>
</hibernate-mapping>