<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    

      <RelativeLayout
        android:id="@+id/wharfwork_view_titleR"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/title_backgrundmenu" >

        <ImageButton
            android:id="@+id/wharfwork_view_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:background="#00000000"
            android:contentDescription="@string/back"
            android:src="@drawable/title_back" />

        <TextView
            android:id="@+id/wharfwork_view_title"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/wharfwork_view_back"
            android:gravity="fill_vertical"
            android:text="@string/wharfwork_button"
            android:textColor="#0067ac"
            android:textSize="18sp"/>
    </RelativeLayout> 
    
       
          
       <com.baidu.mapapi.map.MapView
        android:id="@+id/wharfwork_view1_map"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_below="@+id/wharfwork_view_titleR" >
    </com.baidu.mapapi.map.MapView>   
          
          
       
     
       
       
       


</RelativeLayout>