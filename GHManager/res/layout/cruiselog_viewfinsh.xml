<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/addresslist_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical" >

    <TableLayout
        android:id="@+id/cruiselog_viewtiteltable"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/title_backgrundmenu"
        android:orientation="horizontal"
        android:stretchColumns="1" >

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <ImageButton
                android:id="@+id/cruiselog_viewback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:contentDescription="@string/back"
                android:src="@drawable/title_back" />

            <TextView
                android:id="@+id/cruiselog_viewtitle"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="@string/cruiselog_title"
                android:textColor="#0067ac"
                android:textSize="18sp" />
        </TableRow>
    </TableLayout>

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cruiselog_viewtiteltable" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TableLayout
                android:id="@+id/cruiselog_viewtiteltable1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/grey"
                android:focusableInTouchMode="true"
                android:orientation="horizontal"
                android:shrinkColumns="1"
                android:stretchColumns="1" >

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="0.5dp" >

                    <TextView
                        android:id="@+id/cruiselog_cruiseLogLoaction"
                        android:layout_width="wrap_content"
                        android:layout_height="48dp"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:text="@string/cruiselog_cruiseLogLoaction"
                        android:textColor="@color/Light_gray"
                        android:textSize="18sp" >
                    </TextView>

                    <TextView
                        android:id="@+id/cruiselog_cruiseLogLoactionNeirong"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:textSize="18sp" >
                    </TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="0.5dp" >

                    <TextView
                        android:id="@+id/cruiselog_cruiseLogUserName"
                        android:layout_width="wrap_content"
                        android:layout_height="48dp"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:text="@string/cruiselog_cruiseLogUserName"
                        android:textColor="@color/Light_gray"
                        android:textSize="18sp" >
                    </TextView>

                    <TextView
                        android:id="@+id/cruiselog_cruiseLogUserNameNeirong"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:textSize="18sp" >
                    </TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="0.5dp" >

                    <TextView
                        android:id="@+id/cruiselog_shipName"
                        android:layout_width="wrap_content"
                        android:layout_height="48dp"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:text="@string/cruiselog_shipName"
                        android:textColor="@color/Light_gray"
                        android:textSize="18sp" >
                    </TextView>

                    <TextView
                        android:id="@+id/cruiselog_shipNameNeirong"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:textSize="18sp" >
                    </TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="0.5dp" >

                    <TextView
                        android:id="@+id/cruiselog_startTime"
                        android:layout_width="wrap_content"
                        android:layout_height="48dp"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:paddingLeft="10dp"
                        android:text="@string/cruiselog_startTime"
                        android:textColor="@color/Light_gray"
                        android:textSize="18sp" >
                    </TextView>

                    <TextView
                        android:id="@+id/cruiselog_startTimeNeirong"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:textSize="18sp" >
                    </TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="0.5dp" >

                    <TextView
                        android:id="@+id/cruiselog_endTime"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:paddingBottom="10dp"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:text="@string/cruiselog_endTime"
                        android:textColor="@color/Light_gray"
                        android:textSize="18sp" >
                    </TextView>

                    <TextView
                        android:id="@+id/cruiselog_endTimeNeirong"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:paddingBottom="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:textSize="18sp" >
                    </TextView>
                </TableRow>
            </TableLayout>
      <TextView
               android:id="@+id/cruiselog_shijian"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:background="@color/grey"
                 android:layout_below="@+id/cruiselog_viewtiteltable1"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:text="@string/cruiselog_shijian"
                android:textColor="#333"
                android:textSize="14sp" >
            </TextView>
            
            <com.huzhouport.leaveandovertime.MultiListView
                android:id="@+id/cruiselog_viewlist"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cruiselog_shijian" 
              
                >
            </com.huzhouport.leaveandovertime.MultiListView>

  
            
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>