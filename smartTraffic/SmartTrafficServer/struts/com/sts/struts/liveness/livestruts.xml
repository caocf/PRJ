<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="liveness" extends="cement-interceptor"
		namespace="/">
		<action name="autoAddUserLiveness" class="com.sts.liveness.action.LivenessAction"
			method="autoAddUserLiveness">
			<result type="json" name="success"></result>
			<result name="error" />
		</action>


		<action name="queryLivenessAcount" class="com.sts.liveness.action.LivenessAction"
			method="queryLivenessAcount">
			<result type="json" name="success"></result>
			<result name="error" />
		</action>

	</package>
</struts>    
