<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="company" extends="cement-interceptor" namespace="/company">
		<action name="findCompany" class="com.sts.company.action.CompanyAction"
			method="findCompany">
			<result name="success" type="json"/>
			<result name="error"/>
		</action>
		<action name="findOneCompany" class="com.sts.company.action.CompanyAction"
			method="findOneCompany">
			<result name="success" type="json"/>
			<result name="error"/>
		</action>
	</package>
</struts>    
