<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="complain" extends="cement-interceptor" namespace="/">
		<action name="SaveComplain" class="com.sts.complain.action.ComplainAction"
			method="SaveComplain">
			<result type="json" name="success"><param name="root">saveResult</param></result>
			<result name="error"/>
		</action>
		
		<action name="SearchAllComplainType" class="com.sts.complain.action.ComplainTypeAction"
			method="SearchAllComplainType">
			<result type="json" name="success"></result>
			<result name="error"/>
		</action>
	</package>
</struts>    
