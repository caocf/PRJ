<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sts.debug.model.Debug" table="st_debug">
		<id name="id" column="debugID"></id>
		<property name="type" column="debugType"></property>
		<property name="title" column="debugTitle"></property>
		<property name="content" column="debugContent"></property>
		<property name="contact" column="debugContact"></property>

	</class>
</hibernate-mapping>
