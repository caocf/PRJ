<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sts.smartbus.download.model.Download" table="st_download">
		<id name="id" column="id"></id>
		<property name="ip" column="downloadIp"></property>
		<property name="date" column="downTime"></property>
		<property name="appVersion" column="appVersion"></property>
		<property name="oldVersion" column="oldVersion"></property>
		<property name="equitment" column="equitment"></property>
		<property name="imei" column="imei"></property>

	</class>
</hibernate-mapping>
