<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sts.liveness.model.Liveness" table="st_liveness">
		<id name="livenessId" column="livenessId"></id>
		<property name="ip" column="ip"></property>
		<property name="imei" column="imei"></property>
		<property name="logindate" column="logindate"></property>
		<property name="version" column="version"></property>
		<property name="equitment" column="equitment"></property>

	</class>
</hibernate-mapping>
