<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           
  

	<bean id="companyAction" class="com.sts.company.action.CompanyAction" scope="prototype">
		<property name="companyService" ref="companyService"></property>
	</bean>
	
	<bean id="companyService" class="com.sts.company.service.CompanyService" scope="prototype">
		<property name="companyDao" ref="companyDao"></property>
	</bean>
	<bean id="companyDao" class="com.sts.company.dao.CompanyDao" scope="prototype">
		<property name="hibernateTemplate" ref="hibernateTemplate"></property>
		<property name="oraclHibernateTemplate" ref="hibernateTemplateOracle"></property>
	</bean>


</beans>
