<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="parkingDBAction" class="com.sts.parkingdb.action.ParkingDBAction"
		scope="prototype">
		<property name="parkingDBService" ref="parkingDBService"></property>
	</bean>

	<bean id="parkingDBService" class="com.sts.parkingdb.service.ParkingDBService"
		scope="prototype">
		<property name="parkingDBDao" ref="parkingDBDao"></property>
	</bean>

	<bean id="parkingDBDao" class="com.sts.parkingdb.dao.ParkingDBDao"
		scope="prototype">
		<property name="hibernateTemplate" ref="hibernateTemplateOracle"></property>
	</bean>

</beans>
