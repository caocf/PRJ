<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="parkingdb" extends="cement-interceptor"
		namespace="/">

		<action name="searchParkingByID" class="com.sts.parkingdb.action.ParkingDBAction"
			method="searchParkingByID">
			<result type="json" name="success"></result>
			<result name="error" />
		</action>


		<action name="searchParkingByNameAndType" class="com.sts.parkingdb.action.ParkingDBAction"
			method="searchParkingByNameAndType">
			<result type="json" name="success"></result>
			<result name="error" />
		</action>


		<action name="searchParkingByLoacation" class="com.sts.parkingdb.action.ParkingDBAction"
			method="searchParkingByLoacation">
			<result type="json" name="success"></result>
			<result name="error" />
		</action>

	</package>
</struts>    
