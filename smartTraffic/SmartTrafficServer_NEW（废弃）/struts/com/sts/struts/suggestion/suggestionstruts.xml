<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="suggestion" extends="cement-interceptor" namespace="/">
		<action name="SaveSuggestion" class="com.sts.suggstion.action.SuggestionAction"
			method="SaveSuggestion">
			<result type="json" name="success"><param name="root">saveResult</param></result>
			<result name="error"/>
		</action>
		
		<action name="GetSuggestion" class="com.sts.suggstion.action.SuggestionAction"
			method="GetSuggestion">
			<result type="json" name="success"/>
			<result name="error"/>
		</action>
		
		<action name="SearchAllSuggestionType" class="com.sts.suggstion.action.SuggestionTypeAction"
			method="SearchAllSuggestionType">
			<result type="json" name="success"/>
			<result name="error"/>
		</action>
	</package>
</struts>    
