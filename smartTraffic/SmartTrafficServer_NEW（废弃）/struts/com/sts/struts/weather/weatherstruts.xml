    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="weather" extends="cement-interceptor" namespace="/">
		<action name="TodayWeather" class="com.sts.weather.action.WeatherAction"
			method="TodayWeather">
			<result name="success" type="json"><param name="root">weatherFromBaidu</param></result>
			<result name="error"/>
		</action>
	</package>
</struts>    
