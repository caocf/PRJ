<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="taxi" extends="cement-interceptor" namespace="/">
		<action name="orderTaxi" class="com.sts.taxi.action.TaxiAction"
			method="orderTaxi">
			<result name="success" type="json"/>
			<result name="error"/>
		</action>
		
		<action name="queryOrder" class="com.sts.taxi.action.TaxiAction"
			method="queryOrder">
			<result name="success" type="json"/>
			<result name="error"/>
		</action>

	</package>
</struts>    
