<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="debug" extends="cement-interceptor" namespace="/">
		<action name="SaveDebug" class="com.sts.debug.action.DebugAction"
			method="SaveDebug">
			<result type="json" name="success"><param name="root">saveResult</param></result>
			<result name="error"/>
		</action>
		
		<action name="GetDebugs" class="com.sts.debug.action.DebugAction"
			method="GetDebugs">
			<result type="json" name="success"></result>
			<result name="error"/>
		</action>
	</package>
</struts>    
