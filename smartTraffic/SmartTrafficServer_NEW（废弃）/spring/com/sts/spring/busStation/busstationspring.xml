<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           
  

	<bean id="busStationAction" class="com.sts.station.action.BusStationAction" scope="prototype">
		<property name="busStationService" ref="busStationService"></property>
	</bean>
	
	<bean id="busStationService" class="com.sts.station.service.BusStationService" scope="prototype">
	</bean>
	


</beans>
