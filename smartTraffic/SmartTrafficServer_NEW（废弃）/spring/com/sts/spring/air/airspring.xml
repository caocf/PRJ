<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           
  

	<bean id="airScheduleAction" class="com.sts.air.action.AirScheduleAction" scope="prototype">
		<property name="airScheduleService" ref="airScheduleService"></property>
	</bean>
	
	<bean id="airScheduleService" class="com.sts.air.service.AirScheduleService" scope="prototype">
		<property name="airDao" ref="airDao"></property>
	</bean>
	
	<bean id="airDao" class="com.sts.air.dao.AirDao" scope="prototype">
		<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>

</beans>
