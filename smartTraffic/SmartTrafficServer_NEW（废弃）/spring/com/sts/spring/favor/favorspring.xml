<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">           
  

	<bean id="favorAction" class="com.sts.favor.action.FavorAction" scope="prototype">
		<property name="favorService" ref="favorService"></property>
	</bean>
	
	<bean id="favorService" class="com.sts.favor.service.FavorService" scope="prototype">
		<property name="favorDao" ref="favorDao"></property>
	</bean>
	
	<bean id="favorDao" class="com.sts.favor.dao.FavorDao" scope="prototype">
	<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>

</beans>
