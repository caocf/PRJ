<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>

	  <!--                    操作日志配置信息                                                     -->
	<package name="czrzmanager" extends="cement-interceptor" namespace="/czrzmanager">
	    <!--  获取机日志列表  -->
		<action name="czrzlist" method="selectCzrzList"
			class="com.highwaycenter.log.action.CzrzManagerAction">
			<result name="json" type="json"></result>
		</action>
		 <!--  批量删除日志  -->
		<action name="deleteczrz" method="deleteCzrzList"
			class="com.highwaycenter.log.action.CzrzManagerAction">
			<result name="json" type="json"></result>
		</action>
		 <!--  日志详情  -->
		<action name="czrzxq" method="selectCzrzxq"
			class="com.highwaycenter.log.action.CzrzManagerAction">
			<result name="json" type="json"></result>
		</action>
		 <!--  新增日志  -->
		<action name="czrzsave" method="saveCzrz"
			class="com.highwaycenter.log.action.CzrzManagerAction">
			<result name="json" type="json"></result>
		</action>
		
		 <!--  导出日志excel  -->
		<action name="exportexcel" method="exportExcel"
			class="com.highwaycenter.log.action.CzrzManagerAction">
			<result name="json" type="json"></result>
			
		</action>
	</package>
</struts>    