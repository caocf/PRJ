<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.highwaycenter.data.model.HSnopeLog" table="h_snope_log"  >
        <id name="jobid" type="java.lang.Integer">
            <column name="JOBID" />
            <generator class="identity" />
        </id>
        <property name="jobname" type="java.lang.String">
            <column name="JOBNAME" />  
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS" />  
        </property>
        <property name="totallineread" type="java.lang.Long">
            <column name="TOTALLINEREAD" />  
        </property>
         <property name="totallinewritten" type="java.lang.Long">
            <column name="TOTALLINEWRITTEN" />  
        </property>
         <property name="totallineupdate" type="java.lang.Long">
            <column name="TOTALLINEUPDATED" />  
        </property>
        <property name="totallinerejected" type="java.lang.Long">
            <column name="TOTALLINEREJECTED" />  
        </property>
        <property name="errors" type="java.lang.Long">
            <column name="ERRORS" />  
        </property>
        <property name="startdate" type="java.lang.String">
            <column name="STARTDATE" />  
        </property>
         <property name="enddate" type="java.lang.String">
            <column name="ENDDATE" />  
        </property>
        <property name="translog" type="java.lang.String">
            <column name="TRANSLOG" />  
        </property>
         <property name="steplog" type="java.lang.String">
            <column name="STEPLOG" />  
        </property>
        
    </class>
</hibernate-mapping>
