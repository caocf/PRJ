<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.SmartTraffic.video.model.STControlUnit" table="z_st_control"  >
        <id name="controlUnitID" type="java.lang.Integer">
            <column name="controlUnit_id"/>
            <generator class="assigned" />
        </id>
        <property name="controlUnitName" type="java.lang.String">
            <column name="controlUnit_name"  />
        </property>
         <property name="upControlUnitID" type="java.lang.Integer">
            <column name="upControlUnit_id"  />
        </property>
        
        </class>
</hibernate-mapping>