		<action name="tbGlvideo" method="tbGlvideo"
			class="com.SmartTraffic.video.action.VideoAction">
			<result name="success" type="json"></result>
		</action>
		<action name="tbVideo" method="tbVideo"
			class="com.SmartTraffic.video.action.VideoAction">
			<result name="success" type="json"></result>
		</action>
