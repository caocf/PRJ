<?xml version="1.0" encoding="UTF-8"?>
<ext-config>
<textField type="hidden" id="year"/>

			<table>
				<tr>
					<td>
						<text>
<![CDATA[
<div style='font-size:16px; text-align:center;margin-top:10%;margin-botton:0%'>
公路养护里程按行政区划饼图分析
<a href='javascript:clickme(2006);'></a> 
</div>

	]]>
</text>
					<chart shape="pie" xcol="mname" ycol="val"  width="355"
						height="360">
						<property>
							<key name="ydesc" value="公路里程" />
							<key name="xdesc" value="行政区划" />
							<key name="formatCol" value="###,##0.000" />
							<key name="unitCol" value="公里" />
							<key name="showLegend" value="true"/>
							<key name="showLabel" value="true"/>
							<key name="action" value="clickme" />
							<key name="legendLayout" value="horizontal" />
						</property>
						<ds><![CDATA[
select xzqh, b.mname,  sum(JS_ZLC) val from MDM_GL_ZLC_LN a, mcode_xzqh_dtl b where a.xzqh = b.mid
#if($year !='')
          and year = '$year'
#end

group by XZQH, b.mname,  px order by px asc

						]]></ds>
					</chart>	
					</td>
					
					<td>
						<text>
<![CDATA[
<div style='font-size:16px; text-align:center;margin-top:10%;margin-botton:0%'>
公路养护里程按行等级划饼图分析
<a href='javascript:clickme(2007);'></a> 
</div>

	]]>
</text>
					<chart shape="pie" xcol="mname" ycol="val" width="340"
						height="360">
						<property>
							<key name="ydesc" value="公路里程" />
							<key name="xdesc" value="行政等级" />
							<key name="formatCol" value="###,##0.000" />
							<key name="unitCol" value="公里" />
							<key name="showLegend" value="true"/>
							<key name="showLabel" value="true"/>
							<key name="action" value="clickme" />
                                                        <key name="legendLayout" value="horizontal" />
						</property>
						<ds><![CDATA[
select gl_xzdj, b.mname,  sum(XZ_ZLC) val from MDM_GL_ZLC_LN a, mcode_gl_xzdj b where a.gl_xzdj = b.mid
#if($year !='')
          and year = '$year'
#end


group by gl_xzdj, b.mname,  gl_xzdj order by gl_xzdj asc
						]]></ds>
					</chart>
					</td>
					
					<td>
						<text>
<![CDATA[
<div style='font-size:16px; text-align:center;margin-top:10%;margin-botton:0%'>
公路养护里程按技术等级饼图分析
<a href='javascript:clickme(2007);'></a> 
</div>

	]]>
</text>
						<chart shape="pie" xcol="mname" ycol="val" width="360"
						height="340">
						<property>
							<key name="ydesc" value="公路里程" />
							<key name="xdesc" value="技术等级" />
							<key name="formatCol" value="###,##0.000" />
							<key name="unitCol" value="公里" />
							<key name="showLegend" value="true"/>
							<key name="showLabel" value="true"/>
							<key name="action" value="clickme" />
                                                        
                                                        <key name="legendLayout" value="horizontal" />
						</property>
						<ds><![CDATA[
select gl_jsdj, b.mname,  sum(JS_ZLC) val from MDM_GL_ZLC_LN a, mcode_gl_jsdj b where a.gl_jsdj = b.mid
#if($year !='')
          and year = '$year'
#end


group by gl_jsdj, b.mname,  gl_jsdj order by gl_jsdj asc
						]]></ds>
					</chart>
					</td>
				</tr>
			</table>	
</ext-config>
